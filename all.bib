	Doi = {10.1086/307784},
	Eprint = {astro-ph/9809177},
	Journal = {\apj},
	Keywords = {ISM: CLOUDS, ISM: KINEMATICS AND DYNAMICS, ISM: MAGNETIC FIELDS, MAGNETOHYDRODYNAMICS: MHD, TURBULENCE, ISM: Clouds, ISM: Kinematics and Dynamics, ISM: Magnetic Fields, Magnetohydrodynamics: MHD, Turbulence},
	Month = oct,
