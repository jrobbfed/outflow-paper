	Doi = {10.1086/498672},
	Eprint = {astro-ph/0510221},
	Journal = {\apj},
	Keywords = {Galaxies: Elliptical and Lenticular, cD, Galaxies: Formation, Galaxies: Fundamental Parameters, Galaxies: Structure},
	Month = feb,
	Pages = {725-738},
	Title = {{The Fundamental Manifold of Spheroids}},
