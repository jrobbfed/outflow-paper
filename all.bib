	Keywords = {Interstellar Extinction, Interstellar Matter, Open Clusters, Stellar Luminosity, Stellar Mass, Stellar Temperature, Stellar Winds, Hertzsprung-Russell Diagram, Image Analysis, Spectrum Analysis, Star Distribution, Stellar Models, Stellar Spectrophotometry},
