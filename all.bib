	Eprint = {1111.0592},
	Journal = {\mnras},
	Keywords = {stars: luminosity function, mass function, stars: statistics, galaxies: stellar content},
	Month = feb,
	Pages = {1183-1187},
	Primaryclass = {astro-ph.GA},
	Title = {{Probabilistic distributions of M/L values for ultrafaint dwarf spheroidal galaxies: stochastic samplings of the initial mass function}},
	Volume = 420,
	Year = 2012,
