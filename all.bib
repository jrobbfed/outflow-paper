	Keywords = {GALAXIES: EVOLUTION, GALAXIES: INDIVIDUAL: M82, GALAXIES: STARBURST, GALAXIES: STAR CLUSTERS, GALAXIES: STELLAR CONTENT},
	Month = sep,
