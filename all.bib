	Journal = {\apj},
	Keywords = {Galaxy: Globular Clusters: General, Stars: Luminosity Function, Mass Function},
	Month = may,
	Pages = {870-879},
	Title = {{On the Globular Cluster Initial Mass Function below 1 M$_{solar}$}},
