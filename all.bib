	Date-Added = {2014-04-25 16:58:56 +0000},
	Date-Modified = {2018-07-10 16:52:29 +0000},
	Doi = {10.1051/0004-6361:200809394},
	Eprint = {0805.1218},
	Journal = {\aap},
