	Doi = {10.1093/mnras/stt866},
	Eprint = {1301.3905},
	Journal = {\mnras},
	Keywords = {stars: formation, galaxies: evolution, galaxies: formation, galaxies: high-redshift, galaxies: ISM, galaxies: starburst},
	Month = aug,
	Pages = {1970-1990},
