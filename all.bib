	Journal = {ArXiv e-prints},
	Keywords = {Astrophysics - Cosmology and Nongalactic Astrophysics},
	Month = may,
	Title = {{Introducing the Illustris Project: Simulating the coevolution of dark and visible matter in the Universe}},
