	Date-Modified = {2018-07-10 16:52:29 +0000},
	Doi = {10.1086/591786},
	Eprint = {0807.1533},
	Journal = {\apj},
	Keywords = {Cosmology: Observations, Galaxies: Evolution, Galaxies: Formation},
	Month = oct,
	Pages = {1503-1513},
	Title = {{EAZY: A Fast, Public Photometric Redshift Code}},
