	Author = {{Nelson}, E. and {van Dokkum}, P. and {Franx}, M. and {Brammer}, G. and {Momcheva}, I. and {F{\"o}rster Schreiber}, N. and {da Cunha}, E. and {Tacconi}, L. and {Bezanson}, R. and {Kirkpatrick}, A. and {Leja}, J. and {Rix}, H.-W. and {Skelton}, R. and {van der Wel}, A. and {Whitaker}, K. and {Wuyts}, S.},
	Date-Added = {2014-06-16 12:27:42 +0000},
	Date-Modified = {2018-07-10 16:52:29 +0000},
	Eprint = {1406.3350},
	Journal = {ArXiv e-prints},
	Keywords = {Astrophysics - Astrophysics of Galaxies},
	Month = jun,
	Title = {{A Massive Galaxy in its Core Formation Phase Three Billion Years After the Big Bang}},
	Year = 2014,
	Bdsk-File-1 = {},
