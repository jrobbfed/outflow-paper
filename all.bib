	Doi = {10.1051/0004-6361:20079240},
	Eprint = {0804.4831},
	Journal = {\aap},
	Keywords = {Galaxy: disk, Galaxy: halo, Galaxy: structure, Galaxy: kinematics and dynamics, ISM: structure},
	Month = sep,
	Pages = {951-963},
	Title = {{Global properties of the H I distribution in the outer Milky Way. Planar and extra-planar gas}},
