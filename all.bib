	Eprint = {1505.03855},
	Journal = {ArXiv e-prints},
	Keywords = {Astrophysics - Solar and Stellar Astrophysics},
	Month = may,
	Primaryclass = {astro-ph.SR},
	Title = {{Observational Diagnostics of Self-Gravitating MHD Turbulence in Giant Molecular Clouds}},
	Year = 2015,
	Bdsk-File-1 = {},
