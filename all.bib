	Doi = {10.1093/mnras/stt508},
	Eprint = {1303.4995},
	Journal = {\mnras},
	Keywords = {stars: formation, ISM: clouds, ISM: evolution, galaxies: ISM},
	Month = jun,
	Pages = {653-667},
	Primaryclass = {astro-ph.GA},
	Title = {{The exciting lives of giant molecular clouds}},
