	Author = {{Rubin}, R.~H. and {Simpson}, J.~P. and {O'Dell}, C.~R. and {McNabb}, I.~A. and {Colgan}, S.~W.~J. and {Zhuge}, S.~Y. and {Ferland}, G.~J. and {Hidalgo}, S.~A.},
