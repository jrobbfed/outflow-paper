	Archiveprefix = {arXiv},
	Arxivurl = {http://arXiv.org/abs/1302.2679},
	Author = {{Ripple}, F. and {Heyer}, M.~H. and {Gutermuth}, R. and {Snell}, R.~L. and {Brunt}, C.~M.},
	Date-Added = {2015-07-02 20:10:34 +0000},
	Date-Modified = {2018-07-10 16:52:29 +0000},
	Doi = {10.1093/mnras/stt247},
	Eprint = {1302.2679},
	Journal = {\mnras},
