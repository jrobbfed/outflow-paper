	Keywords = {STARS: FORMATION, STARS: LUMINOSITY FUNCTION, MASS FUNCTION, GALAXIES: EVOLUTION, GALAXIES: FORMATION, GALAXIES: STELLAR CONTENT, DARK MATTER},
