	Eprint = {1002.1239},
	Journal = {\aap},
	Keywords = {magnetohydrodynamics (MHD), turbulence, techniques: spectroscopic, ISM: molecules, radio lines: ISM, ISM: kinematics and dynamics},
	Month = apr,
	Pages = {A67},
	Title = {{The density variance - Mach number relation in the Taurus molecular cloud}},
