	Journal = {ArXiv e-prints},
	Keywords = {Astrophysics - Galaxy Astrophysics},
	Month = mar,
	Primaryclass = {astro-ph.GA},
	Title = {{The Density Variance--Mach Number Relation in Supersonic Turbulence: I. Isothermal, magnetised gas}},
