	Journal = {\apj},
	Keywords = {Carbon Monoxide, H Ii Regions, Interstellar Gas, Nebulae, Stellar Evolution, Stellar Spectra, Early Stars, Emission Spectra, Infrared Astronomy, Line Spectra, Molecular Gases, Radial Velocity, Radio Emission, Shock Fronts, T Tauri Stars},
	Month = jul,
