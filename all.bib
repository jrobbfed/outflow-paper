	Journal = {\apj},
	Keywords = {Elliptical Galaxies, Mathematical Models, Stellar Models, Stellar Motions, Stellar Structure, Stellar Systems, Density Distribution, Dynamic Stability, Gravitational Fields, Integral Equations, Linear Programming, Orbital Mechanics},
