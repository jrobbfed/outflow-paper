	Author = {{La Barbera}, F. and {Ferreras}, I. and {Vazdekis}, A. and {de la Rosa}, I.~G. and {de Carvalho}, R.~R. and {Trevisan}, M. and {Falc{\'o}n-Barroso}, J. and {Ricciardelli}, E.},
	Date-Added = {2014-05-06 20:48:13 +0000},
	Date-Modified = {2018-07-10 16:52:29 +0000},
	Doi = {10.1093/mnras/stt943},
	Eprint = {1305.2273},
