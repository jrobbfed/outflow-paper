	Doi = {10.1093/mnras/stv941},
	Eprint = {1504.03690},
	Journal = {\mnras},
	Keywords = {MHD, turbulence, stars: formation, ISM: clouds, ISM: jets and outflows, ISM: kinematics and dynamics},
