	Eprint = {astro-ph/9910156},
	Journal = {\aap},
	Keywords = {ISM: CLOUDS, ISM: INDIVIDUAL OBJECTS: ORION A, ISM: JETS AND OUTFLOWS, STARS: FORMATION, STARS: MASS-LOSS},
