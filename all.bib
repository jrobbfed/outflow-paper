	Author = {{Hartwig}, T. and {Bromm}, V. and {Klessen}, R.~S. and {Glover}, S.~C.~O.},
	Date-Added = {2014-11-06 14:46:42 +0000},
	Date-Modified = {2018-07-10 16:52:29 +0000},
	Eprint = {1411.1238},
	Journal = {ArXiv e-prints},
	Keywords = {Astrophysics - Astrophysics of Galaxies},
	Month = nov,
	Title = {{Constraining the primordial initial mass function with stellar archaeology}},
	Year = 2014,
	Bdsk-File-1 = {},
